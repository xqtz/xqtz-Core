setclipboard("https://discord.gg/QZwnfkKS8Y")

--loading speed

game.StarterGui:SetCore("SendNotification", {
   Title = "Script",
   Text = "Loading script...",
   Icon = "",
   Duration = 5
})

wait(1)

local start = os.clock()

-- gui

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("xqtz Core", "Midnight")

--info

local info = Window:NewTab("xqtz Core for Arms_of_Conquest")
local info = info:NewSection("made with love, xqtz")

info:NewLabel("join discord to make suggestions what to add")
info:NewLabel("mobile/xbox doesn't get a custom musket")

--no bullet drop
 
local Main = Window:NewTab("bullet drop")
local MainSection = Main:NewSection("no bullet drop")
 
 
MainSection:NewButton("no bullet drop", "Halberd", function()
   game.Players.LocalPlayer.Backpack["Land Pattern Pistol"].Settings.Miscellaneous.BulletDropPerFrame.Value = 0
end)
 
MainSection:NewButton("no bullet drop (mobile)", "(mobile)", function()
   game.Players.LocalPlayer.Backpack["1766 Charleville Musket"].Settings.Miscellaneous.BulletDropPerFrame.Value = 0
end)

MainSection:NewButton("no bullet drop", "Officer", function()
   game.Players.LocalPlayer.Backpack["Land Pattern Pistol "].Settings.Miscellaneous.BulletDropPerFrame.Value = 0
end)

MainSection:NewButton("no bullet drop ", "Ranker", function()
   game.Players.LocalPlayer.Backpack["1766 Charleville Musket"].Settings.Miscellaneous.BulletDropPerFrame.Value = 0
end)

MainSection:NewButton("no bullet drop (Xbox)", "Xbox", function()
   game.Players.LocalPlayer.Backpack["1766 Charleville Musket"].Settings.Miscellaneous.BulletDropPerFrame.Value = 0
end)

MainSection:NewButton("no bullet drop", "Ranker", function()
    local StarterGui = game:GetService("StarterGui")
    local player = game.Players.LocalPlayer

    local tool = player.Backpack:FindFirstChild("1766 Charleville Musket")
    if not tool then return end

    tool.Settings.Miscellaneous.BulletDropPerFrame.Value = 0

    StarterGui:SetCore("SendNotification", {
        Title = "Success",
        Text = "Bullet drop disabled ðŸŽ¯",
        Duration = 3
    })
end)


 


--cred

local Credits = Window:NewTab("Credits")
local Credits = Credits:NewSection("creds")
 
Credits:NewLabel("Owner xqtz")
Credits:NewLabel("tester 4fa")
Credits:NewLabel("https://discord.gg/QZwnfkKS8Y")

--fin

local finish = os.clock()

local time = finish - start
local rating
if time < 3 then
   rating = "fast"
elseif time < 5 then
   rating = "acceptable"
else
   rating = "slow"
end

game.StarterGui:SetCore("SendNotification", {
   Title = "Script",
   Text = string.format("Script loaded in %.2f seconds (%s loading)", time, rating),
   Icon = "",
   Duration = 5
})
